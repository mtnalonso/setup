export EDITOR="/usr/bin/vim"

alias glog='git log --oneline --graph --decorate'
alias gs="git status"

alias l='exa --oneline'
alias ls='exa'
alias ll='exa --long'
alias la='exa --all'
alias lr='exa --recurse'
alias lt='exa --tree'
alias lh='exa -bghHliS' # huge mode
alias lla='exa --long --all'

alias weather="curl http://wttr.in/"
alias moon="curl wttr.in/Moon"

